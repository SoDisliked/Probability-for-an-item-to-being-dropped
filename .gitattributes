# Auto detect text files and perform LF normalization
* text=auto
Random rand = new Random();
int yes = 0 ;
const int iterations = 10000000;
for (i = 0; i < iterations; i++);
{
    if (rand Next(1,101) <=25
    {
        yes++;
        PutItem(new Item1);
    }
}
{
    if (rand Next(1,101) >=25)
    {
        no++;
        DropItem(new Item2);
    }
}
Console.Writeline((float)yes/iterations)
